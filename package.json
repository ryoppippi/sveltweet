{
	"name": "sveltweet",
	"type": "module",
	"version": "0.5.1",
	"packageManager": "pnpm@10.20.0",
	"description": "The best way to embed tweets in your Svelte app, supporting both SSR and static prerendering modes.",
	"author": "ryoppippi",
	"license": "MIT",
	"funding": "https://github.com/ryoppippi/sveltweet?sponsor=1",
	"repository": "ryoppippi/sveltweet.git",
	"sideEffects": false,
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": "22.15.1",
			"onFail": "download"
		}
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./api": {
			"types": "./dist/api.d.ts",
			"import": "./dist/api.js"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"!dist/**/*.spec.*",
		"!dist/**/*.test.*",
		"dist",
		"src/lib/"
	],
	"svelte": "./dist/index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm /^build:/",
		"build:page": "vite build",
		"build:package": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --cache .",
		"format": "pnpm lint --fix",
		"prerelease": "pnpm /^prerelease:/",
		"prerelease:check": "pnpm check",
		"prerelease:lint": "pnpm lint",
		"prerelease:build": "pnpm build && publint",
		"release": "bumpp",
		"prepare": "pnpm /^prepare:/",
		"prepare:copy": "./copy.mjs",
		"prepare:git": "simple-git-hooks",
		"prepack": "pnpm build && clean-pkg-json -r scripts.preinstall",
		"preinstall": "npx only-allow pnpm"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.48.3",
		"svelte": "^5.43.0"
	},
	"devDependencies": {
		"@ryoppippi/eslint-config": "^0.4.0",
		"@ryoppippi/svelte-preprocess-import-css": "jsr:^0.3.4",
		"@std/async": "jsr:^1.0.12",
		"@sveltejs/adapter-vercel": "^6.1.1",
		"@sveltejs/kit": "^2.48.3",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"bumpp": "^10.3.1",
		"clean-pkg-json": "^1.3.0",
		"dax-sh": "^0.43.2",
		"eslint": "^9.38.0",
		"eslint-plugin-format": "^1.0.2",
		"eslint-plugin-svelte": "^3.12.5",
		"lint-staged": "^16.2.6",
		"magic-string": "^0.30.21",
		"publint": "^0.3.15",
		"react-tweet": "3.2.2",
		"simple-git-hooks": "^2.13.1",
		"std-env": "^3.10.0",
		"svelte": "^5.43.0",
		"svelte-check": "^4.3.3",
		"svelte-eslint-parser": "^1.4.0",
		"svelte-preprocess-cssmodules": "^3.0.1",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-svelte-plugin": "^0.3.50",
		"valibot": "^1.1.0",
		"vite": "^7.1.12"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*": [
			"eslint --cache --fix"
		]
	}
}
