{
	"name": "sveltweet",
	"type": "module",
	"version": "0.2.3",
	"packageManager": "pnpm@9.7.1",
	"description": "The best way to embed tweets in your Svelte app, supporting both SSR and static prerendering modes.",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/sveltweet",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./api": {
			"types": "./dist/api.d.ts",
			"import": "./dist/api.js"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"!dist/**/*.spec.*",
		"!dist/**/*.test.*",
		"dist",
		"src/lib/"
	],
	"svelte": "./dist/index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "nr build:page && nr build:package",
		"build:page": "vite build",
		"build:package": "svelte-kit sync && svelte-package && nlx publint",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"release": "nr check && nr lint && nr build && nlx bumpp && npm publish",
		"prepare": "./copy.mjs"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.5.24",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@antfu/ni": "^0.22.4",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.23",
		"@ryoppippi/svelte-preprocess-import-css": "npm:@jsr/ryoppippi__svelte-preprocess-import-css@^0.3.4",
		"@std/async": "npm:@jsr/std__async@^1.0.3",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/kit": "^2.5.24",
		"@sveltejs/package": "^2.3.4",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.6",
		"dax-sh": "^0.41.0",
		"eslint": "^9.9.1",
		"eslint-plugin-format": "^0.1.2",
		"eslint-plugin-svelte": "^2.43.0",
		"magic-string": "^0.30.11",
		"react-tweet": "^3.2.1",
		"std-env": "^3.7.0",
		"svelte": "5.0.0-next.237",
		"svelte-check": "^3.8.6",
		"svelte-eslint-parser": "^0.41.0",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"typescript-svelte-plugin": "^0.3.40",
		"vite": "^5.4.1"
	}
}
